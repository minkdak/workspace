  private static Car createTico() {
    Engine e1 = new Engine(8, 4);
    
    Tire[] tireList = {
      new Tire("비트타이어", 13),
      new Tire("비트타이어", 13),
      new Tire("비트타이어", 13),
      new Tire("비트타이어", 13)
    };
    
    Car c1 = new Car();
    c1.setModel("티코");
    c1.setCc(800);
    c1.setEngine(e1);
    c1.setTires(tireList);
    
    return c1;
  }
  
