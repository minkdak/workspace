/* Mybatis 퍼시스턴스 프레임워크: 사용
 * 1) mybatis 라이브러리 파일 가져오기
 *    - mvnrepository.com "mybatis" 검색
 */
package step28.ex2;

import java.util.List;

public class Test01 {
  public static void main(String[] args) throws Exception {
    DBConnectionPool conPool = new DBConnectionPool(

    "com.mysql.jdbc.Driver",
    "jdbc:mysql://localhost:3306/webappdb",
    "webapp",
    "1111");
    MemberDaoImpl memberDao = new MemberDaoImpl();
    memberDao.setDBConPool(conPool);
    
    List<Member> list = memberDao.selectList(1, 5);
    for (Member m : list) {
      System.out.printf("%d, %s, %s, %s\n", m.getNo(), m.getName(), m.getEmail(), m.getTel());
    }
  }
}
