package step27.ex2;

public class Engine {
  int valve;
  int cylindar;
  
  
  @Override
  public String toString() {
    return "Engine [valve=" + valve + ", cylindar=" + cylindar + "]";
  }
  
  public int getValve() {
    return valve;
  }
  public void setValve(int valve) {
    this.valve = valve;
  }
  public int getCylindar() {
    return cylindar;
  }
  public void setCylindar(int cylindar) {
    this.cylindar = cylindar;
  }
  
  
}
