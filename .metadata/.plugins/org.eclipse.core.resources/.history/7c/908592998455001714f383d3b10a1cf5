package step27.ex2;

public class Engine {
  int valve;
  int cylinder;
  
  public Engine() {}
  public Engine(int valve, int ctylindar) {
    this.valve = valve;
    this.cylindar = cylinder;
  }
  @Override
  public String toString() {
    return "Engine [valve=" + valve + ", cylinder=" + cylinder + "]";
  }
  
  public int getValve() {
    return valve;
  }
  public void setValve(int valve) {
    this.valve = valve;
  }
  public int getCylindar() {
    return cylinder;
  }
  public void setCylindar(int cylinder) {
    this.cylinder = cylinder;
  }
  
  
}
